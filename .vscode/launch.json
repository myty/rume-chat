  "version": "0.2.0",
  "configurations": [
    {
      "request": "launch",
      "name": "Dev - API",
      "type": "node",
      "cwd": "${workspaceFolder}/apps/api",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--inspect-wait",
        "--env",
        "--unstable-kv",
        "-A",
        "--watch",
        "./main.ts"
      ],
      "console": "integratedTerminal",
      "env": {},
      "attachSimplePort": 9229
    }
  ]
}
