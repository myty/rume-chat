{
  "name": "@myty/rume-chat-cli",
  "exports": {},
  "tasks": {
    "dev:cli": "deno run -A --unstable-kv main.tsx"
  },
  "compilerOptions": {
    "strict": true,
    "lib": [
      "dom",
      "dom.iterable",
      "ES2020"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "@types/react",
    "noImplicitAny": false
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "react": "npm:react@^19",
    "ink": "npm:ink@^6"
  }
}
