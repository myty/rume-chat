{
  "name": "@myty/fresh-workspace-web",
  "version": "0.0.0",
  "exports": {},
  "tasks": {
    "dev": "deno run -A --node-modules-dir npm:vite",
    "build": "deno run -A --node-modules-dir npm:vite build",
    "preview": "deno run -A --node-modules-dir npm:vite preview",
    "serve": "deno run --allow-net --allow-read jsr:@std/http@1/file-server dist/"
  },
  "compilerOptions": {
    "strict": true,
    "lib": [
      "dom",
      "dom.iterable",
      "ES2020"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "@types/react",
    "noImplicitAny": false
  },
  "exclude": [
    ".vite"
  ],
  "imports": {
    "axios": "npm:axios@1.8.4",
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.4",
    "@headlessui/react": "npm:@headlessui/react@^2.2.1",
    "@heroicons/react": "npm:@heroicons/react@^2.2.0",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.3",
    "@tanstack/react-query": "npm:@tanstack/react-query@^5.72.2",
    "@tanstack/react-query-devtools": "npm:@tanstack/react-query-devtools@^5.72.2",
    "@tanstack/react-router": "npm:@tanstack/react-router@^1.115.2",
    "@tanstack/react-store": "npm:@tanstack/react-store@^0.7.0",
    "@tanstack/router-devtools": "npm:@tanstack/router-devtools@^1.115.2",
    "@tanstack/router-plugin": "npm:@tanstack/router-plugin@^1.115.2",
    "@types/react": "npm:@types/react@^19.1.0",
    "@types/react-dom": "npm:@types/react-dom@^19.1.2",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.3.4",
    "class-variance-authority": "npm:class-variance-authority@^0.7.1",
    "clsx": "npm:clsx@^2.1.1",
    "react": "npm:react@^19.1.0",
    "react-dom": "npm:react-dom@^19.1.0",
    "tailwindcss": "npm:tailwindcss@^4.1.3",
    "use-stick-to-bottom": "npm:use-stick-to-bottom@^1.1.0",
    "vite": "npm:vite@^6.2.6"
  }
}
